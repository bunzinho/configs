	"tokenColors":[
        {
            "name": "keyword and control",
            "scope":["keyword","keyword.control"],
            "settings": {
                "foreground": "#c191ff"
            }
        },
        {
            "name": "keyword operator",
            "scope":["keyword.operator"],
            "settings": {
                "foreground": "#ffeec0"
            }
        },
        {
            "name": "storage",
            "scope":["storage"],
            "settings": {
                "foreground": "#b8e993"
            }
        },
        {
            "name": "storage modifier array",
            "scope":["storage.modifier.array"],
            "settings": {
                "foreground": "#ffeec0"
            }
        },
        {
            "name": "functions",
            "scope":["entity.name.function","support.function"],
            "settings": {
                "foreground": "#ffd166"
            }
        },
        {
            "name": "entity name",
            "scope":["entity.name"],
            "settings": {
                "foreground": "#D24858"
            }
        },
        {
            "name": "variables",
            "scope":["variable","variable.other.object"],
            "settings": {
                "foreground": "#d27448"
            }
        },
        {
            "name": "variable paramters",
            "scope":["variable.parameter"],
            "settings": {
                "foreground": "#8dcae6"
            }
        },
        {
            "name": "constants",
            "scope":["constant"],
            "settings": {
                "foreground": "#ecaad2"
            }
        },
        {
            "name": "strings",
            "scope":["string"],
            "settings": {
                "foreground": "#83aa83"
            }
        },
        {
            "name": "comment",
            "scope":["comment"],
            "settings": {
                "foreground": "#5B5E66"
            }
        },
        {
            "name": "support type primitive",

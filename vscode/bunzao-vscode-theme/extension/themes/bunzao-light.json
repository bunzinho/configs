	"tokenColors":[
        {
            "name": "keyword and control",
            "scope":["keyword","keyword.control"],
            "settings": {
                "foreground": "#3333c0"
            }
        },
        {
            "name": "keyword operator",
            "scope":["keyword.operator"],
            "settings": {
                "foreground": "#010101"
            }
        },
        {
            "name": "storage",
            "scope":["storage"],
            "settings": {
                "foreground": "#d03040"
            }
        },
        {
            "name": "storage modifier array",
            "scope":["storage.modifier.array"],
            "settings": {
                "foreground": "#010101"
            }
        },
        {
            "name": "functions",
            "scope":["entity.name.function","support.function"],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "entity name",
            "scope":["entity.name"],
            "settings": {
                "foreground": "#d03040"
            }
        },
        {
            "name": "variables",
            "scope":["variable","variable.other.object"],
            "settings": {
                "foreground": "#a74900"
            }
        },
        {
            "name": "variable paramters",
            "scope":["variable.parameter"],
            "settings": {
                "foreground": "#8dcae6"
            }
        },
        {
            "name": "constants",
            "scope":["constant", "variable.other.constant"],
            "settings": {
                "foreground": "#a000a0"
            }
        },
        {
            "name": "strings",
            "scope":["string"],
            "settings": {
                "foreground": "#009000"
            }
        },
        {
            "name": "comment",
            "scope":["comment"],
            "settings": {
                "foreground": "#83aa83"
            }
        },
        {
            "name": "support type primitive",
            "scope":["support.type.primitive"],
            "settings": {
                "foreground": "#d03040"
            }
        },
        {
            "name": "support type property name",
            "scope":["support.type.property-name"],
            "settings": {
                "foreground": "#a74900"
            }
        }
    ]
}
